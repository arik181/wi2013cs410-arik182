
body {
  background-color: #000000;
  color: #ffffff;
  font-family: sans-serif;
  font-size: 14px;
  font-weight: normal;
  line-height: 1.2em;
  margin: 15px;
}

#container {
  width: 600px;
  height: 600px;
  position: relative;
  margin-right: auto;
  margin-left: auto;
  border: 20px solid #ffffff;
  background-color: #999999;
}

#text {
  color: #ff0000;
  font-size: 30pt;
  font-weight: bold;
  text-align: center;
  margin-top: 140px;
  z-index: 20;
}
